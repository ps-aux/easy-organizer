<!DOCTYPE ui:UiBinder SYSTEM 
"http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:mwd="urn:import:sk.lkcm.organizer.client.view">

    <ui:style field="style">
        .container{
            margin:auto;
            margin-top: 30px;
            width:1000px;
            height:400px;
            padding:10px;
        }
    </ui:style>
    <ui:with field="res"
        type="sk.lkcm.organizer.client.Resources" />
    <g:VerticalPanel ui:field="panel " styleName="{style.container}
         {res.css.rounded}">
    </g:VerticalPanel>
</ui:UiBinder>